package step5;

import java.util.*;

public class no2675 {

	public static void main(String[] args) {
		// 문자열 반복
		// 각 문자를 반복하여 출력하는 문제
		
		Scanner scanner = new Scanner(System.in);
		
		int T = scanner.nextInt();
		
		
		for (int i = 0; i < T; i++) {
			int R = scanner.nextInt();
			String S = scanner.nextLine();
			
			System.out.println(R);
			System.out.println(S);
			
		}
		

	}

}
