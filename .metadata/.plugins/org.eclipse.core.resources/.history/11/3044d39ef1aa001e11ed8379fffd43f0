package step8;

import java.util.*;

public class no2869 {

	public static void main(String[] args) {
		// 달팽이는 올라가고 싶다
		// 달팽이의 움직임을 계산하는 문제
		
		Scanner scanner = new Scanner(System.in);
		
		int A = scanner.nextInt();
		int B = scanner.nextInt();
		int V = scanner.nextInt();
		int count = 0;
		
		while (A < V) {
			A = A - B;
			count++;
		}
		System.out.println(count);
	}

}
