package step6;

import java.util.*;

public class no2444 {

	public static void main(String[] args) {
		// º° Âï±â - 7
		
		Scanner scanner = new Scanner(System.in);
		
		int N = scanner.nextInt();
		
		for (int line = 0; line < 2*N - 1; line++) {
			for (int i = 0; i < line + 1; i = i + 2) {
				System.out.print("*");
			}
			System.out.println();
		}

	}

}
