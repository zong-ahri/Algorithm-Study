package step9;

import java.util.*;

public class no9506 {

	public static void main(String[] args) {
		// 약수들의 합
		// 약수를 구하면서 주어진 수가 완전수인지 판별하는 문제
		
		Scanner scanner = new Scanner(System.in);
		
		List<Integer> array = new ArrayList<>();
		String[] str = {};
		int count = 0;
		
		while (true) {
			int A = scanner.nextInt();
			int sum = 0;
			if (A > 0) {
				for (int i = 1; i < A; i++) {
					if (A % i == 0) {
						System.out.print(i + " ");
						sum += i;
					}
					System.out.println();
				}
				System.out.println(sum);
				
				
			} else 
				break;
			
		}
		
		
		

	}

}
