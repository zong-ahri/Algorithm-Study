package step12;

import java.io.*;
import java.util.*;

public class no1436 {
	public static void main(String[] args) {
		// 영화감독 숌
		// N번째 종말의 수가 나올 때까지 차례대로 시도하는 문제
		
		Scanner scanner = new Scanner(System.in);
		BufferedReader br = new BufferedReader(new InputStreamReaker(System.in));
		
		int N = scanner.nextInt();
		
		int num = 666;
		int count = 1;
		
		while (count != N) {
			num++;
			if (String.valueOf(num).contains("666")) {
				count++;
			}
		}
		
		System.out.println(num);
		
	}
}
