package step5;

import java.util.Arrays;
import java.util.Scanner;

public class no2908 {

	public static void main(String[] args) {
		// 상수
		// 숫자를 뒤집어서 비교하는 문제
		
		Scanner sc = new Scanner(System.in);
		
		String[] Ary1 = sc.next().split("");
		String[] Ary2 = sc.next().split("");
		String temp;
		
		temp = Ary1[0];
		Ary1[0] = Ary1[2];
		Ary1[2] = temp;
		
		System.out.println(Arrays.toString(Ary1));
		
		
		for (int i = 0; i < 3; i++) {
		}
		
		System.out.println(Arrays.toString(Ary1));
		System.out.println(Arrays.toString(Ary2));
		System.out.println(Ary1[0].getClass().getTypeName());
	}

}
