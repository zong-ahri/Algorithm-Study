package step11;

import java.util.*;

public class no24262 {

	public static void main(String[] args) {
		// 알고리즘 수업 - 알고리즘의 수행 시간 1
		// 프로그램의 수행 시간을 분석해 봅시다.
		
		Scanner scanner = new Scanner(System.in);
		
		int number = scanner.nextInt();
		int count = 0;
		
		while (true) {
			number = number / 2;
			count++;
			if (number == 0)
				break;
		}

	}

}
