package step9;

import java.util.*;

public class no9506 {

	public static void main(String[] args) {
		// 약수들의 합
		// 약수를 구하면서 주어진 수가 완전수인지 판별하는 문제
		
		Scanner scanner = new Scanner(System.in);
		
		List<Integer> array = new ArrayList<>();
		List<String> str = new ArrayList<>();
		int count = 0;
		
		while (true) {
			int A = scanner.nextInt();
			int sum = 0;
			if (A > 0) {
				for (int i = 1; i < A; i++) {
					if (A % i == 0) {
						System.out.print(i + " ");
						array.add(i) ;
						sum += i;
					}
				}
				System.out.println();
				System.out.println("약수의 합: " + sum);
				System.out.println(A + "의 약수: " + array);
				
				if (A == sum) {
//					System.out.print(A + " = " + str[count].trim().replace(" ", "+"));
				}
				
				
			} else 
				break;
			
		}
		
		
		

	}

}
