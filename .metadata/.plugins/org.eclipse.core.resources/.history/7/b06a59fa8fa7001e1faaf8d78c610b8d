package step5;

import java.util.Arrays;
import java.util.Scanner;

public class no2908 {

	public static void main(String[] args) {
		// 상수
		// 숫자를 뒤집어서 비교하는 문제
		
		Scanner sc = new Scanner(System.in);
		
		String[] aAry = sc.next().split("");
		String[] bAry = sc.next().split("");
		String temp;
		
		temp = aAry[0];
		aAry[0] = aAry[2];
		aAry[2] = temp;
		
		temp = bAry[0];
		bAry[0] = bAry[2];
		bAry[2] = temp;

		for (int i = 0; i < 3; i++) {
			System.out.print(aAry[i]);
		}
		
		
	}

}
