package step5;

import java.util.Arrays;
import java.util.Scanner;

public class no2908 {

	public static void main(String[] args) {
		// 상수
		// 숫자를 뒤집어서 비교하는 문제
		
		Scanner sc = new Scanner(System.in);
		
		String[] aAry = sc.next().split("");
		String[] bAry = sc.next().split("");
		String aTemp;
		String bTemp;
		
		aTemp = aAry[0];
		aAry[0] = aAry[2];
		aAry[2] = aTemp;
		
		bTemp = bAry[0];
		bAry[0] = bAry[2];
		bAry[2] = bTemp;

		aTemp = "";
		bTemp = "";
		
		for (int i = 0; i < 3; i++) {
			aTemp += aAry[i];
			bTemp += bAry[i];
		}
		
		System.out.print(aTemp + " " + bTemp);
		
		if (Integer.parseInt(aTemp) > Integer.parseInt(bTemp))
			System.out.println(aTemp);
		else
			System.out.println(bTemp);
		
	}

}
