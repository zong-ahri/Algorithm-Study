package step8;

import java.util.*;

public class no2292 {

	public static void main(String[] args) {
		// 벌집
		// 벌집이 형성되는 규칙에 따라 벌집의 위치를 구하는 문제
		
		Scanner scanner = new Scanner(System.in);
		
		int value = 6;
		int number = scanner.nextInt();
		int count = 0;
		
		
		while (number > 0) {
			number = number / value;
			System.out.println(number);
			count++;
		}
		System.out.println(count);

	}

}
