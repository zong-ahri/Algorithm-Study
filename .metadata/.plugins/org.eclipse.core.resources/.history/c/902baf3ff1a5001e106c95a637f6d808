package No.step4;

import java.util.Scanner;

public class no3052 {

	public static void main(String[] args) {
		// 나머지
		// 배열을 활용하여 서로 다른 값의 개수를 찾는 문제
		
		Scanner scanner = new Scanner(System.in);
		
		int[] numbers = new int[10];
		final int fNum = 42;
		int count = 0;
		
		// 나머지 배열에 삽입
		for (int i = 0; i < numbers.length; i++) {
			numbers[i] = scanner.nextInt();
			numbers[i] = numbers[i] % fNum;
		}
		
		System.out.println("나머지 배열을 보여줍니다");
		for (int i = 0; i < numbers.length; i++) {
			System.out.println(numbers[i]);
		}

		// 서로 다른 나머지 몇 개 있는지 출력
		for (int i = 0; i < numbers.length-1; i++) { 		// i는 인데스 0~8까지
			for (int j = i+1; i < numbers.length; j++) {	// j는 인덱스 1~9까지
				if (numbers[i] != numbers[j]) {		// 비교한 값이 서로 다르면 카운팅
					count++;
				} else {
					numbers[j] = -1;
				}
			}
			numbers[i] = -1;
		}
		
		System.out.println(count);

	}

}
