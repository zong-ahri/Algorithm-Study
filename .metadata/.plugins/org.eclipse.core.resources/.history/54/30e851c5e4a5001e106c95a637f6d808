package No.step4;

import java.util.Scanner;

public class no5597 {

	public static void main(String[] args) {
		// 과제 안 내신 분..?
		// 과제 제출 기한이 지났습니다.

		Scanner scanner = new Scanner(System.in);
		
		int[] n = new int[30];
		int 제출한사람 = 28;
		
		
		System.out.println(n.length);
		
		for (int i = 0; i < n.length; i++) {
			n[i] = 0;
		}
		
		for(int i = 1; i < 제출한사람+1; i++) {
			int submit = scanner.nextInt();
			
			n[submit] = 1;
		}
		
//		for(int i = 0; i < n.length; i++) {
//		}
		
		
	}

}
