package step9;

import java.util.Scanner;

public class no9506 {

	public static void main(String[] args) {
		// 약수들의 합
		// 약수를 구하면서 주어진 수가 완전수인지 판별하는 문제
		
		Scanner scanner = new Scanner(System.in);
		
		while (true) {
			int A = scanner.nextInt();
			if (A > 0) {
				System.out.print(A + " = ");
				for (int i = 1; i < A; i++) {
					if (A % i == 0) {
						System.out.print(i + " +");
					}
				}
				System.out.println();
			} else {
				break;
			}
		}
		
		
		

	}

}
