package step8;

import java.util.*;

public class no2720 {

	public static void main(String[] args) {
		// 세탁소 사장 동혁
		

		
		Scanner scanner = new Scanner(System.in);
		
		int T; // 케이스 개수
		int C; // 거스름돈 (단위는 센트)
		
		T = scanner.nextInt();
		for (int i = 0; i < T; i++) {
			C = scanner.nextInt();
			function(C);
		}
		
		

	}
	
	private static void function(int c) {
		
		final int quarter = 25;
		final int dime = 10;
		final int nickel = 5;
		final int penny = 1;
		int remainder = 0; // 나머지
		
		List<Integer> result = new ArrayList<>();
		
		while (c > 0) {
			if (c >= quarter) {
				c /= quarter;
				remainder = c % quarter;
				System.out.print(c);
				System.out.print(remainder);
			} else if (c >= dime && c < quarter) {
				c /= dime;
				remainder = c % dime;
				System.out.print(c);
				System.out.print(remainder);
			} else if (c >= nickel && c < dime) {
				c /= nickel;
				remainder = c % nickel;
				System.out.print(c);
				System.out.print(remainder);
			} else if (c >= penny && c < nickel) {
				c /= penny;
				remainder = c % penny;
				System.out.print(c);
				System.out.print(remainder);
			} else
				break;
			System.out.println();
		}
		
	}

}
