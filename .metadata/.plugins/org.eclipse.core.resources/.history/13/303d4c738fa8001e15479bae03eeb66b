package step6;

import java.util.*;

public class no2444 {

	public static void main(String[] args) {
		// º° Âï±â - 7
		
		Scanner scanner = new Scanner(System.in);
		
		int N = scanner.nextInt()+1;
		
		for (int line = 1; line < N; line++) {
			for (int i = line; i > N; i--) {
				System.out.print("*");
			}
			System.out.println();
		}

	}

}
