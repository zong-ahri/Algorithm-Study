package step8;

import java.util.*;

public class no2720 {

	public static void main(String[] args) {
		// 세탁소 사장 동혁
		

		
		Scanner scanner = new Scanner(System.in);
		
		int T; // 케이스 개수
		int C; // 거스름돈 (단위는 센트)
		
		T = scanner.nextInt();
		for (int i = 0; i < T; i++) {
			C = scanner.nextInt();
			function(C);
		}
		
		

	}
	
	private static void function(int C) {
		
		final int quarter = 25;
		final int dime = 10;
		final int nickel = 5;
		final int penny = 1;
		int quotient = 0; // 몫
		int remainder = 0; // 나머지
		
		List<Integer> result = new ArrayList<>();
		
		while (C > 0) {
			if (C >= quarter) {
				quotient = C / quarter;
				C = remainder = C % quarter;
				System.out.print(quotient +" :1) ");
				System.out.print(remainder +" .1) ");
				result.add(quotient);
			} else if (C >= dime && C < quarter) {
				quotient = C / dime;
				C = remainder = C % dime;
				System.out.print(quotient+" :2) ");
				System.out.print(remainder+" .2) ");
				result.add(quotient);
			} else if (C >= nickel && C < dime) {
				quotient = C / nickel;
				C = remainder = C % nickel;
				System.out.print(quotient+" :3) ");
				System.out.print(remainder+" .3) ");
				result.add(quotient);
			} else if (C >= penny && C < nickel) {
				quotient = C / penny;
				C = remainder = C % penny;
				System.out.print(quotient+" :4) ");
				System.out.print(remainder+" .4) ");
				result.add(quotient);
			} else
				break;
			System.out.println();
		}
		System.out.println(result);
		
	}

}
