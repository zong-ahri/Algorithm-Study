package step5;

import java.util.*;

public class no1152 {

	public static void main(String[] args) {
		// 단어의 개수
		// 단어의 개수를 구하는 문제
		
		Scanner sc = new Scanner(System.in);
		
		String str = sc.nextLine();
		
		String[] word = str.split(" ");

		System.out.print(Arrays.toString(word));
		
		System.out.println(word.length);
		
//		if (word[0] == "") {
