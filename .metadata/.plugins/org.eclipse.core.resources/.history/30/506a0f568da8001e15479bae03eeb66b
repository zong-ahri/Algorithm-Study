package step6;

import java.util.*;

public class no2444 {

	public static void main(String[] args) {
		// º° Âï±â - 7
		
		Scanner scanner = new Scanner(System.in);
		
		int N = scanner.nextInt();
		
		for (int i = 0; i < 2*N-1; i++) {
			for (int j = i; j < N; j++) {
				System.out.print("*");
			}
			System.out.println();
		}

	}

}
