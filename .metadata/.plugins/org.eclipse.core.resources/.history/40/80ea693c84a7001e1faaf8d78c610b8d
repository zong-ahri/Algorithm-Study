package step5;

import java.util.*;

public class no1152 {

	public static void main(String[] args) {
		// 단어의 개수
		// 단어의 개수를 구하는 문제
		
		Scanner sc = new Scanner(System.in);
		
		String str = sc.nextLine();
		
		if (str.isEmpty()) {
			System.out.println(0);
			return;
		}
		
		String[] word = str.split(" ");

		System.out.println(word.length);
		
//		if (word[0] == "") {
//			System.out.println(word.length-1);
//		} else {
//			System.out.println(word.length);
//		}

	}

}
