package step8;

import java.util.*;

public class no11005 {

	public static void main(String[] args) {
		// 진법 변환 2
		// 반대 방향으로 진법을 변환하는 문제
		
		// 10진법 수 N을 B진법으로 바꿔 출력하는 프로그램
		
		Map<Character, Integer> base = new HashMap<>();
		for (int i = 0; i < 36; i++) {
			if (i < 10) {
				base.put((char)(i+48), i);
			} else {
				base.put((char)(i+55), i);
			}
		}
		
		System.out.println(base);
		

	}

}
