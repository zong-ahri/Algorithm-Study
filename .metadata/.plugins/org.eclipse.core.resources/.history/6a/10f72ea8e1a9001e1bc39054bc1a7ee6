package step7;

import java.util.*;

public class no2738 {

	public static void main(String[] args) {
		// 행렬 덧셈
		// 행렬을 2차원 배열로 만들어 더하는 문제
		
		Scanner scanner = new Scanner(System.in);
		
		int column = scanner.nextInt(); // 행
		int row = scanner.nextInt(); // 열
		
		int[][] matrix = new int[column][row];
		
		for (int i = 0; i < column; i++) {
			for (int j = 0; j < row; j++) {
				System.out.println("*");
			}
			System.out.println();
		}
		

	}

}
